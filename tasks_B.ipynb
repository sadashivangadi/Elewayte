   "outputs": [],
   "source": [
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# Load dataset\n",
    "df = pd.read_csv(\"Housing.csv\")\n",
    "\n",
    "# Convert rupees → lakhs\n",
    "df['price_lakhs'] = df['price'] / 100000\n",
    "\n",
    "# Group by Air Conditioning (0 = No, 1 = Yes)\n",
    "avg_price_ac = df.groupby('airconditioning')['price_lakhs'].mean()\n",
    "print(\"Average Price by AC Availability:\\n\", avg_price_ac)\n",
    "\n",
    "# Plot\n",
    "plt.figure(figsize=(6,4))\n",
    "plt.bar(avg_price_ac.index.astype(str), avg_price_ac.values, color=['orange', 'green'])\n",
    "plt.xlabel(\"Air Conditioning (0 = No, 1 = Yes)\")\n",
    "plt.ylabel(\"Average Price (Lakhs)\")\n",
    "plt.title(\"Average House Price: AC vs Non-AC Houses\")\n",
    "plt.grid(axis='y', linestyle='--', alpha=0.5)\n",
    "plt.show()"
   ]
  }
 ],
